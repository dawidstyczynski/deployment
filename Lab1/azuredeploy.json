{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "gitStorageUri": {
            "type": "string"
        },
        "storageDeploymentName": {
            "type": "string",
            "metadata": {
                "description": "The storage deployment name"
            }
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
    {
        "name": "[parameters('storageDeploymentName')]",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "properties": {
            "mode": "Incremental",
            "templateLink": {
                "uri": "[concat(parameters('gitStorageUri'),'.json')]",
                "contentVersion": "1.0.0.0"
            },
            "parametersLink": {
                "uri": "[concat(parameters('gitStorageUri'),'.parameters.json')]"
            }
        }
    },
    {
        "name": "webappDeploymentKekw213",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "properties": {
            "mode": "Incremental",
            "templateLink": {
                "uri": "[concat('https://raw.githubusercontent.com/dawidstyczynski/deployment/master/Lab1/web-app/azuredeploy','.json')]",
                "contentVersion": "1.0.0.0"
            },
            "parametersLink": {
                "uri": "[concat('https://raw.githubusercontent.com/dawidstyczynski/deployment/master/Lab1/web-app/azuredeploy','.parameters.json')]"
            }
        }
    }
    ],
    "outputs": {}
}